  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.3.0",
    "jest": "^23.4.1",
    "ts-jest": "^23.0.0",
    "typedoc": "^0.11.1",
    "typescript": "^2.9.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
